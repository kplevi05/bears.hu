<script>
    function toggleDiv(divId, gombId) {
      var div = document.getElementById(divId);
      if (div.style.display ==='none') {
        div.style.display = 'block';
      } else {
        div.style.display = 'none';
      }
	var gomb = document.getElementById(gombId);	
		if (gomb.textContent==='V'){
